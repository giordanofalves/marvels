<div class="row">
  <div class="col-lg-12">
      <h1 class="page-header">Characters Gallery</h1>
  </div>

  <% characters["data"]["results"].each do |character| %>
    <div class="col-lg-3 col-md-4 col-xs-6 thumb">
      <a class="thumbnail" href="#">
        <%= image_tag("#{character['thumbnail']['path']}/landscape_large.#{character['thumbnail']['extension']}") %>
        <%= "#{character['name']}" %>
      </a>
    </div>
  <% end %>
</div>
